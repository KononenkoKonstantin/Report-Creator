@model IEnumerable<string>
<ul class="nav nav-pills">

    @foreach (var link in Model)
    {
        <li class="nav-item">
            @Html.RouteLink(link, new { controller = "Expenditure", action = "Index", category = link, page = 1 },
               new { @class = link == ViewBag.SelectedCategory ? "selected nav-link active" : null })
        </li>
    }

</ul>
